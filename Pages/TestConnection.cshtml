@page
@model VONetData.Pages.TestConnectionModel
@{
    ViewData["Title"] = "Test MySQL Connection";
}

<h2>Test MySQL Connection</h2>

@if (Model.ConnectionSuccessful)
{
    <div class="alert alert-success">Connection to MySQL was successful!</div>
}
else
{
    <div class="alert alert-danger">Connection failed: @Model.ErrorMessage</div>
}
